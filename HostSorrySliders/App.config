<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehavior">
					<serviceMetadata httpGetEnabled="true"/>
					<serviceDebug includeExceptionDetailInFaults="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service behaviorConfiguration="mexBehavior" name="ServidorSorrySliders.RegistrarUsuarioServicio">
				<endpoint address="IRegistrarUsuario" binding="netTcpBinding" bindingConfiguration="" contract="ServidorSorrySliders.IRegistrarUsuario"/>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8082"/>
						<add baseAddress="net.tcp://localhost:8081"/>
					</baseAddresses>
				</host>
			</service>
		</services>
	</system.serviceModel>
</configuration>