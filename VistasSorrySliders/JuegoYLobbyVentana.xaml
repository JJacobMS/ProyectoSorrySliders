<Window x:Class="VistasSorrySliders.JuegoYLobbyVentana"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VistasSorrySliders"
        Closing="CerrarVentana"
        mc:Ignorable="d" Height="576" Width="1024">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Recursos/tableroLanzamientoFondo.png"></ImageBrush>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".6*"/>
            <ColumnDefinition Width=".4*"/>
        </Grid.ColumnDefinitions>
        <Frame Grid.Column="0" x:Name="frameLobby" NavigationUIVisibility="Hidden" />
        <Frame Grid.Column="1" x:Name="frameListaAmigos" NavigationUIVisibility="Hidden"/>
        
    </Grid>
</Window>
